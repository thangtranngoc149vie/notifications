{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Notifications": "Host=localhost;Port=5432;Database=fisa;Username=fisa;Password=fisa"
  },
  "Aws": {
    "TopicArn": "arn:aws:sns:ap-southeast-1:123456789012:fisa-notify.fifo",
    "GcmAppArn": "arn:aws:sns:ap-southeast-1:123456789012:app/GCM/fisa-android",
    "ApnsAppArn": "arn:aws:sns:ap-southeast-1:123456789012:app/APNS/fisa-ios",
    "WebAppArn": null,
    "Fifo": true
  },
  "OutboxWorker": {
    "BatchSize": 100,
    "PollIntervalMs": 800,
    "MaxRetryAttempts": 10,
    "BaseRetrySeconds": 5,
    "MaxBackoffSeconds": 300
  },
  "WebNotifications": {
    "Enabled": false,
    "HubPath": "/hubs/notifications",
    "BroadcastMethod": "notificationReceived",
    "UserGroupPrefix": "user-",
    "RequireChannelTag": true,
    "ChannelTag": "web",
    "MaxBatchSize": 100
  }
}
